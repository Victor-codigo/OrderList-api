{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/data-fixtures": "^1.6",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "imagine/imagine": "^1.3",
        "lexik/jwt-authentication-bundle": "^2.18",
        "nelmio/cors-bundle": "^2.4",
        "nixilla/php-jwt": "^0.1.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.7",
        "scienta/doctrine-json-functions": "^5.2",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/event-dispatcher": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/mailgun-mailer": "6.2.*",
        "symfony/messenger": "6.2.*",
        "symfony/mime": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.2.*",
        "symfony/password-hasher": "6.2.*",
        "symfony/process": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/string": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/validator": "6.2.*",
        "symfony/web-link": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/extra-bundle": "^3",
        "twig/twig": "^3"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "infection/extension-installer": false
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Test\\": "tests/",
            "Group\\": "src/Group/",
            "Order\\": "src/Order/",
            "ListOrders\\": "src/ListOrders/",
            "Product\\": "src/Product/",
            "Shop\\": "src/Shop/",
            "User\\": "src/User/",
            "Notification\\": "src/Notification/",
            "Common\\": "src/Common/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Group\\Test\\Unit\\": "src/tests/Unit/Group/",
            "Order\\Test\\Unit\\": "src/tests/Unit/Order/",
            "ListOrders\\Test\\Unit\\": "src/test/Unit/ListOrders/",
            "Product\\Test\\Unit\\": "src/tests/Unit/Product/",
            "Shop\\Test\\Unit\\": "src/tests/Unit/Shop/",
            "User\\Test\\Unit\\": "src/tests/Unit/User/",
            "Notification\\Test\\Unit\\": "src/tests/Unit/Notification/",
            "Common\\Test\\Unit\\": "src/tests/Unit/Common/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "require-dev": {
        "dg/bypass-finals": "^v1.5.1",
        "friendsofphp/php-cs-fixer": "^v3.40.0",
        "hautelook/alice-bundle": "^2.12.2",
        "nelmio/api-doc-bundle": "v4.12.0",
        "phpunit/phpunit": "^9.6.13",
        "rector/rector": "^1.2",
        "symfony/browser-kit": "v6.2.7",
        "symfony/css-selector": "6.2.*",
        "symfony/debug-bundle": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    }
}
